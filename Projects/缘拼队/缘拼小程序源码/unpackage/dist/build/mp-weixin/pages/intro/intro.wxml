<view class="body"><view class="title"><view><text class="{{[type=='food'?'food':type=='travel'?'travel':type=='study'?'study':type=='game'?'game':'none']}}">{{titleType}}</text></view><block wx:if="{{userid==myid}}"><view data-event-opts="{{[['tap',[['delPin',['$event']]]]]}}" class="delete" bindtap="__e"><view class="{{[userid==myid?'one':'one_hide']}}"></view><view class="{{[userid==myid?'two':'two_hide']}}"></view></view></block></view><view class="content"><view class="contentHead"><text class="headTitle">{{title}}</text><view class="contentStatus"><image src="../../static/icon/like.png" mode id="like"></image><text>{{likeCount}}</text><image src="../../static/icon/comment.png" id="comment" mode></image><text>{{commentList.length}}</text></view></view><view class="tag"><view class="{{['tag_box',type=='food'?'tagfood':type=='travel'?'tagtravel':type=='study'?'tagstudy':type=='game'?'taggame':'tagnone']}}">{{dowhat}}</view><view class="{{['tag_box',type=='food'?'tagfood':type=='travel'?'tagtravel':type=='study'?'tagstudy':type=='game'?'taggame':'tagnone']}}">{{sex}}</view></view><view class="content_text">{{content_text}}</view><view class="content_img"><image src="{{imgList}}" mode="aspectFill"></image></view><block wx:if="{{type!=='travel'}}"><view data-event-opts="{{[['tap',[['useMap']]]]}}" class="info_address" bindtap="__e"><view class="address_icon-1"><image src="../../static/icon/address.png" mode></image></view><block wx:if="{{this.type!='travel'}}"><view class="address_content">{{address}}</view></block></view></block><block wx:if="{{type==='travel'}}"><view data-event-opts="{{[['tap',[['useMapPstart']]]]}}" class="info_address" bindtap="__e"><view class="address_icon"><image src="../../static/image/-departure.png" mode></image></view><block wx:if="{{this.type!='travel'}}"><view class="address_content">{{PstartaddName}}</view></block></view></block><block wx:if="{{type==='travel'}}"><view data-event-opts="{{[['tap',[['useMapPend']]]]}}" class="info_address" bindtap="__e"><view class="address_icon"><image src="../../static/image/-landing.png" mode></image></view><block wx:if="{{this.type!='travel'}}"><view class="address_content">{{PendaddName}}</view></block></view></block><block wx:if="{{type==='travel'}}"><view data-event-opts="{{[['tap',[['useMapGo']]]]}}" class="info_address" bindtap="__e"><view class="address_icon-1"><image src="../../static/icon/address.png" mode></image></view><block wx:if="{{this.type!='travel'}}"><view class="address_content">点击进行路线规划</view></block></view></block><view class="info_time"><view class="time_icon"><image src="../../static/icon/time.png" mode></image></view><view class="time_content">{{time}}</view></view></view><view data-event-opts="{{[['tap',[['infoPage',['$event']]]]]}}" class="user" bindtap="__e"><view class="user_left"><image src="{{avatarUrl}}" mode></image></view><view class="user_mid">{{name}}</view><block wx:if="{{type=='food'}}"><view data-event-opts="{{[['tap',[['add',['$event']]]]]}}" class="{{[addStutus?'food_add_hover':'food_add']}}" catchtap="__e"><view class="{{[addStutus?'food_one_hover':'food_one']}}"></view><view class="{{[addStutus?'food_two_hover':'food_two']}}"></view></view></block><block wx:if="{{type=='travel'}}"><view data-event-opts="{{[['tap',[['add',['$event']]]]]}}" class="{{[addStutus?'travel_add_hover':'travel_add']}}" catchtap="__e"><view class="{{[addStutus?'travel_one_hover':'travel_one']}}"></view><view class="{{[addStutus?'travel_two_hover':'travel_two']}}"></view></view></block><block wx:if="{{type=='study'}}"><view data-event-opts="{{[['tap',[['add',['$event']]]]]}}" class="{{[addStutus?'study_add_hover':'study_add']}}" catchtap="__e"><view class="{{[addStutus?'study_one_hover':'study_one']}}"></view><view class="{{[addStutus?'study_two_hover':'study_two']}}"></view></view></block><block wx:if="{{type=='game'}}"><view data-event-opts="{{[['tap',[['add',['$event']]]]]}}" class="{{[addStutus?'play_add_hover':'play_add']}}" catchtap="__e"><view class="{{[addStutus?'play_one_hover':'play_one']}}"></view><view class="{{[addStutus?'play_two_hover':'play_two']}}"></view></view></block></view><view class="{{['comment',type=='food'?'food':type=='travel'?'travel':type=='study'?'study':type=='game'?'game':'none']}}">评论</view><block wx:if="{{commentList!=[]}}"><view class="comment_content"><block wx:for="{{commentList}}" wx:for-item="comment" wx:for-index="index" wx:key="index"><view class="comment_box"><view class="comment_user"><image src="{{comment.Userimg}}" mode></image><text>{{comment.nickname}}</text></view><view class="comment_text"><view class="{{['comment_text_box',type=='food'?'bgfood':type=='travel'?'bgtravel':type=='study'?'bgstudy':type=='game'?'bggame':'bgnone']}}"><text>{{comment.msg}}</text></view></view></view></block><block wx:if="{{commentList==[]}}"><view class="comment_empty">暂无评论~</view></block></view></block><view class="{{[add_comment]}}"><view class="{{[showStatus==true?'placeholder_hover':'placeholder']}}">{{comment_placeholder}}</view><input class="commentBar" type="text" focus="{{focus}}" data-event-opts="{{[['input',[['commentCon',['$event']]]],['blur',[['commentBarChange',['$event']]]],['focus',[['commentFocus',['$event']]]]]}}" value="{{commentContent}}" bindinput="__e" bindblur="__e" bindfocus="__e"/><view data-event-opts="{{[['tap',[['send',['$event']]]]]}}" class="{{[showStatus==true?'commentBtn':'commentBtn_hover']}}" catchtap="__e">评论</view></view><view class="tabbar"><view class="tab_left"><view class="like_icon" hover-class="icon_hover" data-event-opts="{{[['tap',[['addLike',['$event']]]]]}}" bindtap="__e"><image src="{{showLike?'../../static/icon/like_hover.png':'../../static/icon/like.png'}}" mode></image></view><view class="comment_icon" hover-class="icon_hover" data-event-opts="{{[['tap',[['comment',['$event']]]]]}}" bindtap="__e"><image src="../../static/icon/comment.png" mode></image></view><view class="collect_icon" hover-class="icon_hover" data-event-opts="{{[['tap',[['collect',['$event']]]]]}}" bindtap="__e"><block wx:if="{{collectStatus}}"><image src="../../static/icon/collect_hover.png" mode></image></block><block wx:if="{{!collectStatus}}"><image src="../../static/icon/collect.png" mode></image></block></view></view><view class="tab_right"><block wx:if="{{status=='2'}}"><view class="{{['pind',type=='food'?'food':type=='travel'?'travel':type=='study'?'study':type=='game'?'game':'none']}}">已被拼</view></block><block wx:if="{{status=='null'}}"><view data-event-opts="{{[['tap',[['talk',['$event']]]]]}}" class="{{['pin',type=='food'?'bgfood':type=='travel'?'bgtravel':type=='study'?'bgstudy':type=='game'?'bggame':'bgnone']}}" bindtap="__e">想拼</view></block></view></view></view>