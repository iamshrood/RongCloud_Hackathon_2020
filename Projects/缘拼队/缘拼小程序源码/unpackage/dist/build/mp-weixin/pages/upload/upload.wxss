page{background-color:#efeeee}.nav{margin-top:180rpx;height:750rpx;line-height:200rpx;width:100%}.nav ._ul{width:700rpx;height:100%;line-height:100%;margin-left:25rpx;margin-right:25rpx}.nav ._ul ._li{margin:50rpx;width:250rpx;height:250rpx;display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-orient:vertical;-webkit-box-direction:normal;-webkit-flex-direction:column;flex-direction:column;background-color:#efeeee;-webkit-transition:all .5s;transition:all .5s;box-shadow:20rpx 20rpx 30rpx rgba(0,0,0,.1),-20rpx -20rpx 30rpx hsla(0,0%,100%,.1);border-radius:20rpx;-webkit-transition:all .2s;transition:all .2s;float:left}.nav ._ul ._li image{width:65%;height:65%;margin-left:17.5%;margin-right:17.5%;margin-top:5%;margin-bottom:5%}.nav ._ul ._li text{-webkit-align-content:center;align-content:center;margin:10rpx auto;font-size:32rpx;font-weight:700;color:#555}.nav ._ul ._li:hover{box-shadow:0rpx 0rpx 0rpx rgba(0,0,0,.1),0rpx 0rpx 0rpx #fff,inset 20rpx 20rpx 30rpx rgba(0,0,0,.1),inset -20rpx -20rpx 20rpx #fff;-webkit-transition:all .5s;transition:all .5s}.tex{text-align:center}.tex text{font-size:36rpx}